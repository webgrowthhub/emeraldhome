/********* login and sign up pages *******/
.inner-pages {
    margin-top:106px;
}
.login_signup form {
    border: 0px solid #000000;
    padding: 30px;
    margin: -72px 0px 60px;
    box-shadow: 0px 0px 14px #40404091;
    border-radius: 5px;
    background: #fff;
}
.login_signup input {
    height: 50px;
    border-radius: 50px;
    padding: 0 20px;
    font-size: 14px;
    color: #000;
}
.login_signup button, .login_signup a {
    width: 100%;
    border-radius: 50px;
    padding: 10px;
    margin: 15px 0px;
    display: inline-block !important;
}
.login_signup a {
    margin:0px;
    display: inline-block;
}
a.forget_pass {
    padding: 0;
    display: inline !important;
}

.divider {
    position: relative;
    margin: 20px 0px !important;
}
.divider:after {
    width: 100%;
    display: inline-block;
    position: absolute;
    height: 1px;
    background: #cacaca;
    content: '';
    left: 0;
    right: 0;
    top: 12px;
}
.divider span {
    background: #fff;
    position: relative;
    z-index: 9;
    padding: 2px 5px;
    color: #cacaca;
}
.login-bg {
    
    padding: 50px 0px;
    color: #fff;
    min-height: 230px;
    background: #469795 url(../images/background-pt.png);
    background-repeat: no-repeat;
    background-size: 100%;    
}
.login-bg h2 {
    font-size: 46px;
